
.pleasure-select-container {
.pleasure-select {
    display: block;
    position: relative;
    width: 100%;
    border: none;
    height: 40px;
    background: var(--pleasure-input-background);
    box-sizing: border-box;
    text-indent: 10px;
    padding: 0 20px;
    outline: none;
}
&.focused {
.el-input__suffix {
.el-input__icon {
        color: var(--pleasure-input-color-icon) !important;
}
}
}
}
